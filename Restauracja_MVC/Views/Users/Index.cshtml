@model IEnumerable<Restauracja_MVC.Models.Users.UserListItem>

@{
    ViewData["Title"] = "Panel administratora - Użytkownicy";
}

<h1>Panel administratora - Użytkownicy</h1>


<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ID)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Email)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>
                @Html.ActionLink("Edytuj", "Edit", new { item.ID }) |
                @Html.ActionLink("Szczegóły", "Details", new { item.ID }) |
                @Html.ActionLink("Usuń", "Delete", new { item.ID })
            </td>
        </tr>
}
    </tbody>
</table>
